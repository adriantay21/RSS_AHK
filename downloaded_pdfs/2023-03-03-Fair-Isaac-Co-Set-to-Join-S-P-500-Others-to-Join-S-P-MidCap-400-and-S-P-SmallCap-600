// Function to capture button text on click
function captureButtonTextOnClick() {
    var capturedText = '';

    // Event listener for button clicks
    document.addEventListener('click', function(event) {
        var clickedElement = event.target;
        if (clickedElement.tagName === 'BUTTON') {
            capturedText = clickedElement.innerText.trim();
            console.log('Captured Button Text:', capturedText);
          _satellite.setVar("buttonText",capturedText);
        }
    });

  
    
}

